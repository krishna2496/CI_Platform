{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "aws/aws-sdk-php": "^3.155",
        "bschmitt/laravel-amqp": "^2.0.8",
        "dusterio/laravel-plain-sqs": "^0.1.17",
        "firebase/php-jwt": "^5.0",
        "flipbox/lumen-generator": "6.0.11",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "hybridauth/hybridauth": "^3.3",
        "iatstuti/laravel-cascade-soft-deletes": "3.0.0",
        "illuminate/contracts": "7.13.0",
        "illuminate/mail": "7.13.0",
        "illuminate/notifications": "7.13.0",
        "laravel/helpers": "^1.2",
        "laravel/lumen-framework": "^7.0",
        "laravelista/lumen-vendor-publish": "7.0.0",
        "league/flysystem": " ~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nicolaslopezj/searchable": "^1.11",
        "onelogin/php-saml": "^3.4",
        "ramsey/uuid": "^3.8",
        "scssphp/scssphp": "^1.0",
        "squizlabs/php_codesniffer": "^3.4",
        "stripe/stripe-php": "^7.54",
        "therobfonz/laravel-mandrill-driver": "^2.0",
        "torann/geoip": "^1.1",
        "vlucas/phpdotenv": "4.0.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "test": "phpunit"
    },
    "scripts-descriptions": {
        "test": "Run phpunit tests. Just add '--' for phpunit arguments."
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
